{  
    "plugins": [ "jasmine" ],
    "env": {
        "jasmine": true
    },
    "extends" : ["airbnb-base/legacy", "plugin:jasmine/recommended"],
    "rules": {
        "func-names": "off",
        "no-alert": "off",
        "no-console": "off",
        "no-plusplus": "off",
        "vars-on-top": "off",
        "no-multi-spaces": "off",
        "key-spacing": "off",
        "max-len": "off",
        "no-undef": "off",
        "no-unused-vars": "off",
        "jasmine/no-spec-dupes": "off"
    }
}
